<script>
	$(document).ready(function () {
		var $adRandIcon = $("#floatingda_home");

		// 해피빈 레이어를 숨긴다.
		$adRandIcon.hide();

		// 해피빈 레이어의 TOP, LEFT 값을 변경(질문 메뉴 좌측) 후 다시 노출한다.
		setTimeout(function () {
			var $elQuestionContentsArea = $.getSingle("div._questionContentsArea");
			var sTopValue = ($elQuestionContentsArea.existElements() ? ($elQuestionContentsArea.offset().top + $elQuestionContentsArea.height()) : $adRandIcon.offset().top) + "px";
			var sLeftValue = "53%";

			$adRandIcon.css({top : sTopValue, left : sLeftValue}).show();

			if (typeof window.gladsdk !== "undefined" && window.gladsdk.cmd) {
				window.gladsdk.cmd.push(function () {
					window.gladsdk.displayAd("floatingda_home");
				});
			}
		}, 200);
	});
</script>

<div id="floatingda_home" class="ad_randicon" style="display: block;"></div>









