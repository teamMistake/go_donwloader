<script>
	
	var extra = {
		dir_id: '60205',
		doc_id: 450471426,
	}
	if (document.referrer) {
		extra.referer = document.referrer;
		if (document.referrer.match(/search.naver.com/)) {
			extra.tracking_code = 'naver_search';
		}
	}
	sendBaLog(BaPcEventType.QNA_DETAIL_5073, extra);
</script>
