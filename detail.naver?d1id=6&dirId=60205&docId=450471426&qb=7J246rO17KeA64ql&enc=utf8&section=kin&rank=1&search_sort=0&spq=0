<script type="text/javascript">
	$(document).ready(function () {
		
			
			
				$.cachedScript('https://ssl.pstatic.net/static.kin/static/kin-web-pc/2307311412/js/min/detail/naver.kin.pc.detail.BottomQnaListTab.js').done(function () {
					if (naver.kin && naver.kin.pc && naver.kin.pc.detail && typeof naver.kin.pc.detail.BottomQnaListTab !== "undefined" && typeof naver.kin.pc.detail.BottomQnaList !== "undefined") {
						window.oDetailQnaListTab = new naver.kin.pc.detail.BottomQnaListTab({
							oRelatedQnaLstParam: {
								sDetailQnaListAreaId: "detailSearchResultListArea",
								sDetailQnaListTemplateId: "detailSearchResultListTpl",
								sDetailQnaListAjaxUrl: "/ajax/detail/detailSearchListAjax.naver",
								oDetailQnaListAjaxData: {
